<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Description>Use the Umbraco CMS v13 without any templates: This library injects a API-REST service which returns content as JSON.</Description>
		<Copyright>Copyright Â© 2023</Copyright>
		<PackageProjectUrl>www.kreadyf.com</PackageProjectUrl>
		<RepositoryUrl>https://github.com/kreadyf/kdyf.umbraco.headless</RepositoryUrl>
		<AssemblyVersion>1.4.0.0</AssemblyVersion>
		<FileVersion>1.4.0.0</FileVersion>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
		<Version>1.4.0</Version>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<Folder Include="Controllers\" />
		<Folder Include="Interfaces\" />
		<Folder Include="Services\" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Dynamitey" Version="2.0.10.189" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
		<PackageReference Include="Slugify.Core" Version="3.0.0" />
		<PackageReference Include="Umbraco.Cms.Web.Common" Version="13.7.2" />
		<PackageReference Include="Umbraco.Cms.Web.Website" Version="13.7.2" />
	</ItemGroup>
</Project>
